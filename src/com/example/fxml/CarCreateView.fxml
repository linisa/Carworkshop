      <TextField fx:id="tfColour" layoutX="95.0" layoutY="285.0" promptText="Colour" />
      <Label layoutX="36.0" layoutY="26.0" prefHeight="57.0" prefWidth="304.0" text="Fill in the values and select a customer from the list which will be the owner of this car" textAlignment="CENTER" textOverrun="CLIP" wrapText="true">
